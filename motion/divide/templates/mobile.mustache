<%!
{{!
    @template plenumtype_divide/mobile

    This template will render the Plenary meeting divide results for mobile

    Variables required for this template:
    * instance - Plenary meeting instance record

    Variables optional for this template:

    Example context (json):
    {
        "contextid": 10,
        "timecreated": 1715465315,
        "totalnay": 17,
        "totalyea": 20
    }
}}
%>
<%< mod_plenum/mobile %>
    <%$ content %>
        <ion-item>
            <ion-label>
                <h5><%# str %> yeas, plenumtype_divide <%/ str %></h5>
                <p><% totalyea %></p>
                <h5><%# str %> nays, plenumtype_divide <%/ str %></h5>
                <p><% totalnay %></p>
            </ion-label>
        </ion-item>
    <%/ content %>
<%/ mod_plenum/mobile %>
